<section class="section-principal" style="height: auto !important;">
  <div
    style="margin-top: 100px"
    id="carouselExampleCaptions"
    class="carousel carousel-dark slide carousel-fade"
    data-bs-ride="carousel"
    data-bs-interval="6000"
  >
    
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img src="/images/banners/21.jpeg" class="carouselimg d-block" alt="First slide" />
        <div  class="carousel-caption-1">
          <h5 style="font-size: 1.5rem;"><%= slides[0].title %></h5>
        </div>
      </div>
      <div class="carousel-item">
        <img src="/images/banners/avion.jpg" class="d-block" alt="..." />
        <div class="carousel-caption-2" >
          <h5 ><%= slides[1].title %></h5>
          <p class="texto-chico">
            <%= slides[1].description %>
          </p>
        </div>
      </div>
      <div class="carousel-item">
        <img src="/images/banners/3.png" class="d-block" alt="..." />
        <div class="carousel-caption-3" >
          <h5 style="font-size: 1.5rem;"><%= slides[2].title %></h5>
        </div>
      </div>
      <div class="carousel-item">
        <img src="/images/banners/grandes.jpg" class="d-block" alt="..." />
        <div class="carousel-caption-4" >
          <h5 style="font-size: 1.5rem;">
            <%= slides[3].title %>
          </h5>
        </div>
      </div>
        <!-- <div class="carousel-item">
          <img src="/images/banners/abuelo.jpg" class="d-block" alt="..." />
          <div class="carousel-caption">
            <h5>Masticar chicles no elimina el mal aliento</h5>
          </div>
        </div> -->
      
      <!-- <div class="carousel-item">
        <img src="/images/banners/20.jpeg" class="d-block" alt="..." />
        <div class="carousel-caption">
          <h5>Tener Mal Aliento representa una DESVENTAJA SOCIAL</h5>
          
        </div>
      </div> -->
   
      
      
     
     
    </div>
    <button
      class="carousel-control-prev"
      type="button"
      data-bs-target="#carouselExampleCaptions"
      data-bs-slide="prev"
    >
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Previous</span>
    </button>
    <button
      class="carousel-control-next"
      type="button"
      data-bs-target="#carouselExampleCaptions"
      data-bs-slide="next"
    >
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
  </div>

</section>
